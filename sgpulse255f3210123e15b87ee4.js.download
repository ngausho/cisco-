        margin: 0 auto;
        position: relative;
        width: 92%;
    }

    .btn__cluster-container {
        position: relative;
        width: calc(50% - .5%);
        position: relative;
    }

    @media only screen and (max-width: 750px){
        .btn__cluster-container {
            width: calc(100% - .5%);
        }
    }

    .btn-text{
        font-size: var(--font-size-base);
        font-weight: var(--font-weight-regular);
        line-height: calc((var(--line-height-regular)) + 0.3);
        border-radius: var(--border-radius);
    }
    
    .btn__action{
        padding: 1.25rem 0;
        width: 100% !important;
        box-shadow: none;
        background-color: rgb(var(--primary-color));
        color: rgb(var(--white-color));
        float: none;
        margin: 0;
    }
    
    .btn__action:hover {
        background-color: rgb(var(--primary-color-shade));
        color: rgb(var(--white-color));
        transition: background-color .2s ease-in,color .2s ease-in;
    }

    button:not(.is-disabled):not(.is-locked) {
        cursor: pointer;
    }

    .btn__marking{
        position: absolute;
        right: 0.4em;
        top: 50%;
        left: initial;
        transform: translate(0%, -50%);
    }

    .btn__marking material-icon{
        width: 2.625rem;
        height: 2.625rem;
        border-radius: 50%;
        color: rgb(var(--white-color));
        box-shadow: 0 0.125rem 0.375rem rgba(0,0,0,0.4);
        line-height: 1.7;
    }

    :host([dir="rtl"]) .btn__marking{
        right: initial;
        top: 50%;
        left: 1em;
    }


    .review-button-container {
        width: 100%;
        display: flex;
        justify-content: center;
    }

    .review-assessment {
        background-color: transparent;
        color: #3d692b;
        text-decoration: underline;
        padding-bottom: 3rem;
    }

    .submit__anyway-checkbox-container{
        display: flex;
        align-items: center;
        gap: 5px;
        user-select:none;
        background: transparent;
    }
    :host([device="small"]) .submit__anyway-checkbox-container{ 
        gap:2px;
    }
    .submit__anyway-checkbox-container #submit-anyway-checkbox-label{
        color: rgb(var(--item-color-inverted));
    }
    .submit__anyway-wrapper{
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: 1em;
        cursor:pointer;
    }
    :host([device="small"]) .submit__anyway-wrapper{
      padding: 0rem 0.9375rem;
    }
    .submit__anyway-checkbox-icon{
        color: rgb(var(--item-color-inverted));
    }

    .submit__anyway-checkbox-icon[icon="check_box"]{
       color: rgb(var(--primary-color));
    }
