html{
    -webkit-text-size-adjust: 100%;
}
.tabs__widget{
    display: flex;
    flex-direction: column;
    overflow: hidden;
} 
:host([device="large"].vertical-layout-tab) .tabs__widget, :host([device="medium"].vertical-layout-tab) .tabs__widget {
  width: 100%;
}
:host([device="small"]) button, :host([device="medium"]) button{
  margin-bottom: 2px;
}

:host([device="large"].vertical-layout-tab) button{
  margin-bottom: 2px;
}

 .tabs__nav-inner{
  display:flex;
  justify-content: space-between;
}
:host([device="small"]) .tabs__nav-inner{
    display:flex;
    justify-content: space-between;
    flex-direction: column;
  }
  :host([device="medium"]) .tabs__nav-inner{
    display:flex;
    justify-content: space-between;
    flex-direction: column;
  }

 .tabs__nav-item-btn {
    word-break: break-word;
    background-color: rgb(var(--background-color-4));
    color: rgb(var(--item-color-inverted));
    border: 1px solid rgb(var(--item-border-color));
    margin-right: calc(var(--margin-right)*2);
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    padding: calc(var(--padding) / 4);
    background-color: rgb(var(--background-color-4));
    color: rgb(var(--item-color-inverted));
    font-size: var(--font-size-base);
    font-weight:var(--font-weight-regular);
    margin-right: calc(var(--margin-right) - 3.87em);
}
.tabs__nav-item-btn:last-child{
  margin-right: 0;
}

.tabs__content {
    border: 1px solid rgb(var(--item-border-color));
    border-color: rgb(var(--secondary-color)) !important;
    background-color: rgb(var(--item-color));
    box-shadow: none !important;
    z-index: 2;
    border-bottom-left-radius: 6px;
    border-bottom-right-radius: 6px;
    padding: calc(var(--padding) / 4);
}
.tabs__content-inner{
    animation: 2s ease 0s 1 normal none running mymove;
}

.tabs__content-item-title {
    margin-bottom: calc(var(--margin-bottom) / 4);
    font-size:var(--font-size-base);
    line-height:var(--line-height-regular);
}
 .tabs__content-item-body {
    display: inline-block;
    width: 100%;
}
.tabs__content-item-graphic {
    display: inline-block;
    width: 100%;
    float: right;
}
img {
    display: block;
    max-width: 100%;
}

.tabs__nav-item-btn:hover {
    background-color: rgb(var(--background-color-4)) !important;
    border-color: rgb(var(--secondary-color)) !important;
    color: rgb(var(--secondary-color)) !important;
    box-shadow: 0 1px 2px rgba(2,8,20,0.06), 0 2px 8px rgba(2,8,20,0.08) !important;
}
.tabs__nav-item-btn.isSelected{
    background-color: rgb(var(--secondary-color)) !important;
    border-color: rgb(var(--secondary-color)) !important;
    color: rgb(var(--white-color)) !important;
}
@keyframes mymove{
    from {top: 10px;}
    to {top: 0px;}
}

.tabs__item-image-container .iframe-container {
    position: relative;
}

.is-horizontal-layout .tabs__widget {
    flex-direction: column-reverse;
  }

  .tabs__nav-item-btn-inner {
    text-align: left;
    line-height: 1.8;
  }
  :host([dir="rtl"]) .tabs__nav-item-btn-inner {
    text-align: right;
  }
  .tabs__content-inner{
    transform: translateY(20px);
    opacity: 0;
}


.delayed-transform{
      transform: translateY(0px);
      opacity: 1;
      transition: transform 0.5s linear, opacity 0.5s linear;
  }
  
  .tabs__item-image-container .iframe-container .webcomponent{
    background-color: rgb(var(--white-color));
    border-radius:calc(var(--border-radius)*1.6);
  }
  :host([device="small"]) .tabs__content-item-body-inner .table, :host([device="small"]) .tabs__content-item-body-inner table{
    min-width: 0px !important;
    overflow-x: auto;
  }
  :host([device="medium"]) .tabs__content-item-body-inner .table, :host([device="medium"]) .tabs__content-item-body-inner table{
    min-width: 0px !important;
    overflow-x: auto;
  }
  :host([device="large"]) .tabs__content-item-body-inner .table, :host([device="large"]) .tabs__content-item-body-inner table{
    min-width: 0px !important;
    overflow-x: auto;
  }
  code .color-orange {
    color: #fbab18 !important;
  }

  code .color-green {
    color: #6ebe4a !important;
  }

  code .color-purple {
    color: #b45bcf !important;
  }
  
  /* tabs__content_image class added for image and body leyout change */
  .tabs__content_image .tabs__content-item .tabs__content-item-inner{
   display: table; 
}
.tabs__content_image .tabs__content-item .tabs__content-item-inner .tabs__content-item-body{
    display: inline-block;
    width: 48%;
 }
 .tabs__content_image .tabs__content-item .tabs__content-item-inner .tabs__content-item-graphic{
    display: inline-block;
    width: 48%;
    float: right;
 }
 @media only screen and (min-width: 5rem) and (max-width: 47.4375rem){
 .tabs__content_image .tabs__content-item .tabs__content-item-inner{
    display: block;
 }

 .tabs__content_image .tabs__content-item .tabs__content-item-inner .tabs__content-item-body {
      display: inline-block;
      width: 100%;
    }

.tabs__content_image .tabs__content-item .tabs__content-item-inner .tabs__content-item-graphic {
      display: inline-block;
      width: 100%;
      float: none;
    }
}
.webComponent_caption {
  margin: 1.25rem 0rem;
  border: thin silver solid;
  padding: 0.5rem
}

:host(.image-resize) .tabs__content-item-graphic-inner{
  max-width: 40%;
  min-width: 40%;
  margin: 46px auto;
}
.tab-component-table table tr th {
  border: 1px solid #000;
  padding: 0.5rem;
  font-size: .75rem;
  background-color: #000;
  color: #ffffff;
}
:host([theme="dark"]) .tabs__widget .tabs__content-item-graphic-inner{
  background-color: rgb(var(--white-color));
