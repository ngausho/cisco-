    .review-container {
        position: Fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 3em;
        z-index: 102;
        transform: translateY(6px);
    }

    .block-buttons-container {
        position: relative;
        height: 3em;
        background-color: rgb(var(--background-color-4));
        z-index: 89;
        display: flex;
        align-items: center;
        width: 100%;
    }

    .block-buttons-container:after {
        content: "";
        width: 1px;
        height: 20px;
        background-color: rgb(var(--background-color-1));
        position: absolute;
        right: -6px;
    }

    .block-buttons-container .btn-icon {
        width: 30px;
        padding: 0px;
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .block-buttons-container .btn-icon.is-left {
        padding-left: 0.2em;
        background-color: rgb(var(--background-color-4));
    }

    .block-buttons-container .btn-icon.is-right {
        padding-right: 0.2em;
        background-color: rgb(var(--background-color-4));
    }

    .review-container .attempted-blocks-wrapper {
        width: 100%;
        overflow: hidden;
    }

    .review-container .highlight{
        background-color: #e2231a20 !important;
        border: 1px solid #E2231A !important;
        color: var(--font-color-1) !important;
    }

    .review-container .highlight.active-block{
        background-color: rgb(var(--neutral-2)) !important;
    }

    .review-container .last-block{
        color: black !important;
    }

    .review-container .attempted-blocks-wrapper .attempted-blocks-inner {
        min-width: 100%;
        margin-left: 0px;
        transition-delay: .3s;
        transition-duration: .2s;
    }

    .review-container .attempted-blocks-wrapper .block-button {
        color: var(--font-color-1);
        margin: 4px;
        text-align: center;
        word-spacing: -2px;
        border: 1px solid #6abf4b;
        border-radius: 4px;
        width: 28px;
        height: 28px;
        box-sizing: border-box;
        font-size: 12px;
        float: initial;
        background-color: #549f3820;
        font-weight: 400;
        line-height: 29px;
    }
    
    .review-container .attempted-blocks-wrapper .block-button:hover {
        background-color: rgba(var(--primary-color), 0.5) !important;
        border: 1px solid rgba(var(--primary-color), 0.5) !important;
        color: rgb(var(--neutral-1)) !important;
    }

    .review-container .attempted-blocks-wrapper .highlight:hover {
        background-color: #e2231a50 !important;
        border: 1px solid #E2231A !important;
    }

    .review-container .attempted-blocks-wrapper .active-block {
        background-color: rgb(var(--neutral-2));
    }

    .review-container .attempted-blocks-wrapper .last-block {
        background-color: rgb(var(--primary-color)) !important;
        font-size: 12px;
        color: #000;
    }

    :host([secure="true"]):focus {
        outline: rgb(var(--secondary-color)) auto 2px !important;
        outline-offset: 3px !important;
    }

    .review-container .attempted-blocks-wrapper .block-button.review-screen-button{
        width: auto !important;
        padding: 0 0.5rem;
        line-height: 29px;
        color: black !important;
    }

    .review-container .attempted-blocks-wrapper .block-button.review-screen-button:hover{
        background-color: rgb(var(--primary-color-shade)) !important;
    }

    .review-container .reset-btn{
        width: auto !important;
        position: fixed;
        bottom: 10px;
        right: 10px;
        padding: calc(var(--padding) / 6);
    }


