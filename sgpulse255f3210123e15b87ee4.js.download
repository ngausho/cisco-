.hide_accordion{
    height:0px !important;
    // display: none;
    overflow: hidden;
    margin-bottom: 0;
    transition: all .4s ease-out;
}

.show{
    overflow: hidden;
    transition: all .4s ease-out;
}

.accordion__item{
    padding-bottom: calc(var(--padding-bottom) - 3.4em); 
}

.accordion__item .accordion__item-btn {
    background-color: rgb(var(--white-color)); 
    color: rgb(var(--black-color));
    border: none;
    display: block;
    margin: 0;
    overflow: hidden;
    position: relative;
    padding: calc(var(--padding) / 3.2);
    text-decoration: none;
    width: 100%;
    text-align: left;
    font-size:100%;
    cursor: pointer;
}

:host([device="small"]) .accordion__item .accordion__item-btn {
    padding: calc(var(--padding-top) / 4) calc(var(--padding-right) / 1.3) calc(var(--padding-bottom) / 4) calc(var(--padding-left) / 4);
}

:host([dir="rtl"]) .accordion__item .accordion__item-btn {
    text-align: right;
}

:host([device="large"]) .align-image-left .accordion__item-body-wrapper, :host([device="medium"]) .align-image-right.accordion__item-body-wrapper{
    display: flex !important;
    flex-direction: row-reverse;
}
:host([device="large"]) .align-image-right .accordion__item-body-wrapper, :host([device="medium"]) .align-image-right.accordion__item-body-wrapper{
    display: flex !important;
    flex-direction: row;
}

.accordion__item .accordion__item-btn {
    background-color: rgb(var(--item-color));
    border-radius: var(--border-radius);
    color: rgb(var(--item-color-inverted));
    border: 1px solid rgb(var(--item-border-color));
}


.accordion__item .accordion__item-btn:hover{
    background-color: rgb(var(--item-color));
    color: rgb(var(--item-color-hover));
    border: 1px solid rgb(var(--item-border-color-hover));
    box-shadow: var(--shadow-1);
    transition: border .5s ease-out,background-color .5s ease-out;
}


.accordion__item .accordion__item-btn.isSelected{
    background-color: rgb(var(--item-color-hover));
    color: rgb(var(--item-color-inverted-hover));
    border: var(--border-width) solid rgb(var(--item-color-hover));
}

.accordion__item .accordion__item-btn:hover material-icon{
    color: rgb(var(--item-color-hover));
}

.accordion__item .accordion__item-btn.isSelected material-icon{
    color: rgb(var( --item-color-inverted-selected));
}

button.accordion__item-btn.isSelected {
    color: rgb(var(--item-color-inverted-selected));
}

button.accordion__item-btn.isSelected material-icon{
    transform: rotate(180deg);
}

.accordion__item .accordion__item-btn .accordion__item-icon{
    position: absolute;
    top: 0%;
    right: 0rem;
    height: 100%;
    padding: 1em;
    transform: rotate(180deg);
    display: flex;
    align-items: center;
}

:host([dir="rtl"]) .accordion__item .accordion__item-btn .accordion__item-icon{
    left: 0rem;
    right: inherit;
}


.accordion__item-body-inner {
    color: rgb(var(--neutral-1));
    border: var(--border-width) solid rgb(var(--item-color-selected));
    margin-top: calc(var(--margin) - 4.3em); 
    border-radius:  0 0 var(--border-radius) var(--border-radius);
    background-color: rgb(var(--background-color-4)); 
    // margin-bottom : calc(var(--margin-bottom) - 3.3em);
}

.accordion__item-body-wrapper{
    padding: calc(var(--padding) - 2.7em) calc(var(--padding) - 1.9em); 
}

/*:host([device="small"]) .accordion__item-body-wrapper{
    padding: calc(var(--padding) / 2 ) calc(var(--padding) / 4);
}*/

.accordion.accordian__content_image .accordion__item-body-wrapper{
    display: flex;
    gap: 4%;
}

:host([dir="rtl"]) .accordion.accordian__content_image .accordion__item-body-wrapper{
    flex-direction: row-reverse;
}

:host([device="medium"]) .accordion.accordian__content_image .accordion__item-body-wrapper{
    display: block;
}

.accordion.accordian__content_image  .accordion__item-body-wrapper .accordion__item-content{
    flex: 1;
}
.accordion.accordian__content_image .accordion__item-body-wrapper .accordion__item-image-container{
    flex: 1;
    max-width: 40%;
}
:host([device="small"]) .accordion.accordian__content_image .accordion__item-body-wrapper .accordion__item-image-container{
    flex: 1;
    max-width: initial;
}

.accordion.accordian__content_image .image-left .accordion__item-body-wrapper{
    flex-direction: row-reverse;
}
:host([device="small"]) .accordion.accordian__content_image .image-left .accordion__item-body-wrapper{
    flex-direction: column-reverse;
}

.accordion.accordian__content_image .image-right .accordion__item-body-wrapper{
    flex-direction: row;
}
:host([device="small"]) .accordion.accordian__content_image .image-right .accordion__item-body-wrapper{
    flex-direction: column;
}

.accordion__item-title-inner{
    word-break: break-word;
}

.hide_accordion{
    margin-top: calc(var(--margin) - 5.3em);
    margin-bottom: 1em;
}


.accordion__item table tbody {
    border: 1px solid rgb(var(--item-border-color));
    background-color: rgb(var(--background-color-3));
    color: rgb(var(--item-color-inverted));
    padding: 1rem;
  }

  .accordion__item-image-container .iframe-container .webcomponent{
    background-color: rgb(var(--primary-color-inverted));
    box-shadow: 0 1px 2px rgba(2,8,20,0.06), 0 2px 8px rgba(2,8,20,0.08);
    border-radius:calc(var(--border-radius)*1.6);
  }
  
.accordion__item-image-container .iframe-container{
    position:relative;
}

.accordion pre code {
    overflow-x: auto !important;
    white-space: nowrap;
}

.hljs {
    border-radius: 0.5rem;
}

code .color-orange {
    color: #fbab18 !important;
}

code .color-green {
    color: #6ebe4a !important;
}

code .color-purple {
    color: #b45bcf !important;
}

.codeinner{
    white-space: normal;
}


/*:host([device="small"]) .accordion pre {
    padding : calc(var(--padding) - 2.75em) calc(var(--padding) - 1.25em) !important;
}*/
:host([device="small"]) .over{
    overflow-x: scroll;
}
:host([device="small"]) .codeinner{
    white-space: nowrap;
}
.image-resize .accordion__item-image-container img{
    max-width: 60%;
    min-width: 60%;
    margin: 46px auto;
}
