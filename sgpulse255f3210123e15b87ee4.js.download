.zoomIn, .zoomOut{
    position: absolute;
    right: 10px;
    top: 10px;
    padding: 4px;
    border: 1px solid #d9d9d9;
    background-color: #fff;
    border-radius: 4px;
    z-index: 5;
    display: flex;
    align-items: center;
    justify-content: center;
    width: 34px;
    height: 34px;
}

:host(.img-target) .zoomIn, .zoomOut{
    top: 90px;
}

.zoomIn{
    padding: 2px;
}

:host([dir="rtl"]) .zoomIn, .zoomOut{
    left: 10px;
    right: initial;
}

:host(.graphic-zoom-image) .zoomIn{
