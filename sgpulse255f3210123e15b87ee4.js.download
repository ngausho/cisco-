.mentor {
    height: 100%;
    position: fixed;
    top: 0;
    width: 100%;
    z-index:500;
}

.mentor:after{
    content: '';
    height:100%;
    position:fixed;
    bottom:0;
    width:100%;
    left:0;
    background-color: rgba(26,32,53,0.8);
    z-index: -1;
    display: block;
}
:host([theme="dark"]) .mentor:after{
    background-color: rgba(42,42,42,0.8);
}

.mentor .mentor__content-inner{
    box-sizing: border-box;
    background-color: rgb(var(--background-color-3));
    color: var(--font-color-1);
    height: auto;
    margin-left: auto;
    margin-right: auto;
    text-align: left;
    width: 100%;
    display: flex;
    flex: 1;
    flex-direction: column;
    padding: 6%;
    border: .0625rem solid rgb(var(--background-color-1));
}

:host([device="large"]) .mentor .mentor__content-inner{
    min-height: 15.625em;
}

.mentor-inner-container{
    position: absolute;
    bottom: 0px;
    display: flex;
}

.mentor .mentor__close{
    position: absolute;
    top: 1px;
    right: 1px;
    border: none;
    padding: 0;
    box-sizing: border-box;
    background: rgb(var(--background-color-2));
    color: var(--font-color-2);
    display: inline-block;
    font-weight: normal;
    height: 3.75rem;
    text-align: center;
    width: 3.75rem;
    transition: all .3s ease-in-out;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor:pointer;
}

.mentor .mentor__content .mentor__close .material-close-icon {
    transform: scale(1.2);
}

:host([theme="dark"]) .mentor .mentor__content .mentor__close .material-close-icon{
    color: rgb(var(--primary-color));
}
.mentor .mentor__content-inner:before{
    border-style: solid;
    content: '';
    display: block;
    height: 0;
    position: absolute;
    width: 0;
    border-color: transparent rgb(var(--background-color-3)) transparent transparent;
    border-width: .9375rem .9375rem .9375rem 0;
    left: 50%;
}

.mentor-inner-container {
    position: relative;
    margin-top: 2.75rem;
    margin-bottom: 4.75rem;
    width: calc(100% - 2.75rem);
    height: 100%;
}

.mentor__image{
    margin-left: auto;
    margin-right: auto;
    z-index: 0;
}

.mentor__inner{
    height: 100%
}

.mentor__content {
    position: relative;
}

@media only screen and (min-width: 47.5rem){

    .mentor-inner-container {
        width: inherit;
        max-width: 50rem;
    }

    .mentor__image{
        margin-left: auto;
        margin-right: auto;
        bottom: 3.75rem;
        left: 0;
        min-width: 51.5%;
        position: absolute;
        width: 51.5%;
    }

    .mentor__content {
        position: absolute;
        right: 0;
        bottom: 25%;
        width: 51.5%;
    }

    .mentor .mentor__content-inner:before{
        left: -0.88rem;
        bottom: 10%;
    }
}

@media only screen and (min-width: 32.5rem) and (max-width: 47.4375rem){
    .mentor .mentor__image {
        width: 80%;
    }
}

@media only screen and (max-width: 47.5rem){
    .mentor__inner {
        display: flex;
        align-items: center;
    }

    .mentor__image {
        min-width: auto;
        width: 70%;
    }

    .mentor-inner-container {
        max-width: 34.375rem;
        display: flex;
        flex-direction: column-reverse;
        margin-left: auto;
        margin-right: auto;
    }

    .mentor .mentor__content-inner:before{
        border-color: rgb(var(--background-color-3)) transparent transparent transparent;
        border-width: 0.9375rem 0.9375rem 0 0.9375rem;
        top: 99%;
        transform: translateX(-50%);
    }

