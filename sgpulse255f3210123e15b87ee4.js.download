    .previous:hover material-icon, .next:hover material-icon{
        color: rgb(var(--secondary-color));
    }
    .narrative__controls-item{
        background-color: rgb(var(--background-color-4));
        border: 1px solid rgb(var(--item-border-color));
        color: rgb(var(--item-color-inverted));
        border-radius: var(--border-radius);
        position: absolute;
        top: 50%;
        z-index: 10;
        transform: translateY(-50%);
        padding: calc(var(--padding) - 3.5em) calc(var(--padding) / 5);
        cursor: pointer;
    }

    .narrative__controls-right {
        right: 0;
        margin-right: 60px
    }
    .narrative__controls-left {
        left: 0;
    }
    .narrative__controls {
        z-index: 10;
    }


    .narrative__text-controls .narrative__content-item .narrative__controls-container {
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        display: flex;
        align-items: center;
        justify-content: space-evenly;
    }

    .narrative__controls-container {
        position: absolute;
        top: 0px;
        right: 0px;
        display: flex;
        width: 44%;
        height: 54px;
    }
    .u-clearfix {
        zoom: 1;
    }

    .narrative .narrative__inner .narrative__widget .narrative__indicators-item {
        width: 50%;
    }

    .narrative__indicators-item {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        width: 86%;
        align-items: center;
    }

    .narrative .narrative__inner .narrative__widget .narrative__content-item {
        width: 50%;
        margin-left: 0%;
    }

    .narrative__text-controls .narrative__content-item {
        position: relative;
        padding-top: 4.05em;
    }


    .narrative__content-item {
        display: block;
        padding-top: 4.05em;
    }
    .narrative__content-inner-item{
        padding: 1.25rem 3.75rem 1.25rem;
    }

    .narrative__content-inner{
        padding: 1.25rem 3.75rem 1.25rem;
    }

    .narrative .narrative__inner .narrative__widget .narrative__content-item .narrative__content-inner-item {
        padding: 1.25rem 3.75rem 1.25rem;
    }

    .narrative .narrative__inner .narrative__widget .narrative__content-item .narrative__content-inner {
        padding: 1.25rem 3.75rem 1.25rem;
    }

    .narrative .narrative__inner .narrative__widget .narrative__indicators-item {
        width: 50%;
    }

    .narrative__text-controls .narrative__content .narrative__indicators-item {
        display: flex;
    }

    .narrative .narrative__inner .narrative__widget .narrative__content .narrative__content-inner {
        padding: 1.25rem 3.75rem 1.25rem;
    }

    .image-resize .narrative__slider-image-container img{
        max-width: 60%;
        min-width: 60%;
        margin: 46px auto;
    }
    :host([theme="dark"]) .webcomponent {
        background-color: rgb(var(--primary-color-inverted));
    }
    .narrative__slider-image-container {
        position: relative;
    }

    .narrative-content-container {
        position: relative;
        overflow: hidden;
    }

    .content-animation-next{
        transform: translateX(100%);
        opacity: 0;
        animation: slide-in .3s linear forwards;
    }

    .content-animation-previous{
        transform: translateX(-100%);
        opacity: 0;
        animation: slide-previous .3s linear forwards;
    }
    
    .narrative__content {
        display: flex;
        transition: transform 5s ease-in-out;
        width: 100%;
    }
    
    .narrative-content {
        min-width: 100%;
        box-sizing: border-box;
        width: 100%;
    }
    
    @keyframes slide-in {
        from {
            transform: translateX(100%);
            opacity: 0;
        }
        to {
            transform: translateX(0);
            opacity: 1;
        }
    }
    
    @keyframes slide-previous {
        from {
            transform: translateX(-100%);
            opacity: 0;
        }
        to {
            transform: translateX(0%);
            opacity: 1;
        }
    }
